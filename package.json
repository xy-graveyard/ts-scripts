  "license": "LGPL-3.0",
  "name": "@xylabs/ts-scripts",
  "peerDependencies": {
    "typescript": "^4"
  },
  "private": false,
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/xylabs/ts-scripts.git"
  },
  "scripts": {
    "build": "rm -r -f bin && yarn lint && yarn lint-pkg && yarn compile",
    "clean": "rm -f *.tsbuildinfo && rm -r -f ./bin",
    "compile": "tsc -p ./tsconfig.build.json",
    "cycle": "eslint --rule 'import/no-cycle: [1, { maxDepth: 10 }]' ./src",
    "dead": "yarn ts-prune",
    "deploy": "yarn build && yarn version --patch && yarn publish",
    "deps": "yarn depcheck",
    "fix": "eslint . --fix",
    "lint": "eslint .",
    "lint-pkg": "npmPkgJsonLint .",
    "reinstall": "rm -r -f ./node_modules && rm -f ./yarn.lock && rm -f ./yarn-error.log && yarn install",
    "up": "yarn ncu",
    "updo": "yarn install && yarn upgrade && yarn ncu -u && yarn reinstall"
  },
  "sideEffects": false,
  "version": "1.0.24"
}
